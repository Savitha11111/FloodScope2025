# FloodScope AI environment variables
# Copy this file to .env and fill in your credentials before running the app.

# Sentinel Hub credentials (required)
SENTINEL_HUB_CLIENT_ID=your_sentinel_hub_client_id_here
SENTINEL_HUB_CLIENT_SECRET=your_sentinel_hub_client_secret_here

# Weather and flood data providers (optional but recommended)
OPENWEATHER_API_KEY=your_openweather_api_key_here
AMBEE_API_KEY=your_ambee_api_key_here

# Additional services
COHERE_API_KEY=your_cohere_api_key_here
GMAIL_USER=your_email_here
GMAIL_APP_PASSWORD=your_gmail_app_password_here
